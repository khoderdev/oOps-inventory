# Server Configuration
PORT=5000
NODE_ENV=development

# Prisma Database URL (ORM approach)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/oops_inventory"

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production
